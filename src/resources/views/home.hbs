<div class="Course">
    <div class="row">
        {{#each course}}
        <div class="col-4 mt-5 " >
            <div class="card " style="height: 450px;">
              <img class="card-img-top" src={{this.image}} alt="">
              <div class="card-body">
                <h4 class="card-title">{{this.name}}</h4>
                <p class="card-text">{{this.description}}</p>
              </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>